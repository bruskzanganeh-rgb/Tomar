    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "npm run lint && npm run typecheck && npm run test:unit",
    "prebuild:ci": "npm run lint && npm run typecheck",
    "test:unit": "vitest run tests/unit",
